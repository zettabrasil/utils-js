{
  "name": "@zettabrasil/utils-js",
  "version": "1.0.0",
  "description": "Common js scripts for formatting and ...",
  "main": "index.js",
  "scripts": {
    "commit": "git-cz",
    "release": "standard-version"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/zettabrasil/utils-js.git"
  },
  "keywords": [
    "utils",
    "js",
    "zettabrasil",
    "formatter",
    "formatting"
  ],
  "author": "Darlei Kroth",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/zettabrasil/utils-js/issues"
  },
  "homepage": "https://github.com/zettabrasil/utils-js#readme",
  "devDependencies": {
    "@commitlint/cli": "^8.3.5",
    "@commitlint/config-conventional": "^8.3.4",
    "commitizen": "^4.1.2",
    "cz-conventional-changelog": "^3.2.0",
    "husky": "^4.2.5",
    "standard-version": "^8.0.0"
  }
}
